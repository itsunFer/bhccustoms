{
    "version": 2,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "public/index.php" }
    ],
    "installCommand": "node build.js",
    "buildCommand": "npm run build",
    "outputDirectory": "public"
  }